<network ipv6='yes'>
  <name>vagrant</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <domain name='vagrant.local' localOnly='yes'/>
  <dns>
    <host ip='10.10.10.1'>
      <hostname>arch</hostname>
    </host>
  </dns>
  <ip address='10.10.10.1' netmask='255.255.255.0'>
    <dhcp>
      <range start='10.10.10.1' end='10.10.10.200'/>
    </dhcp>
  </ip>
</network>

